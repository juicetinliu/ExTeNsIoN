{
    "manifest_version": 2,
    "name": "HeHe",
    "description": "MaKe WoRdS GrEaT AgAiN",
    "version": "1.1",
    "author": "Justin Liu",
    
    "browser_action": {
        "default-icon": "IcOn1.png",
        "default_title": "hEhE"
